{  
  	"name": "Zhihu New Vision",
  	"manifest_version":2,
	"version": "1.0",
	"author": "Leckie", 
	"description": "知乎界面重构 | Only for testing.",  
	"web_accessible_resources":["jquery.min.map"],
	 "content_scripts": [{
	     "matches": ["http://www.zhihu.com/*","https://www.zhihu.com/*"], 
	     "js": ["jquery_1.9.js" , "core.js"], 
	     "css": ["load.css"],
	     "run_at": "document_end",
	     "all_frames": true 
	}],
  	"permissions": [   
  	 "cookies",
   	 "tabs", 
   	 "http://*/*","https://*/*"  
  	]
}